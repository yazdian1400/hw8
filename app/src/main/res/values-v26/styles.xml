<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="styleTextViewTitle" tools:ignore="DuplicateDefinition">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:textColor">#218025</item>
        <item name="android:background">#D9EFD9</item>
        <item name="android:textAlignment">viewEnd</item>
        <item name="android:textSize">24sp</item>
        <item name="android:paddingHorizontal">8dp</item>
    </style>

    <style name="styleEditText1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:selectAllOnFocus">true</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:ems">10</item>
        <item name="android:backgroundTint">@color/pink_500</item>
        <item name="android:textAlignment">viewEnd</item>
        <item name="android:textSize">24sp</item>
        <item name="android:paddingHorizontal">8dp</item>
    </style>

</resources>